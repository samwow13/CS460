
@{
    ViewBag.Title = "GetPage";
}
<!--View for the GetPage() method-->
<center><h2>GetPage Example</h2></center>
<h4>LowerCase Maker</h4>
<div class="container">
    <form>
        <!--Label and input for First Name:-->
        <div>
            <label for="input1">First Name:</label>
            <input type="text" name="input1" value="" />
        </div>
        <!--Label and input for Last Name:-->
        <div>
            <label for="input2">Last Name: </label>
            <input type="text" name="input2" value="" />
        </div>
        <!--Submit button for the text fields above.-->
        <button type="submit" name="submit">Submit</button>

    </form>
</div>
<!--Output area-->
<h3>@ViewBag.caseHeader</h3>
<h4>@ViewBag.additionMessage</h4>

