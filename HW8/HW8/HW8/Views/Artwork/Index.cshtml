@model IEnumerable<HW8.Models.ArtWork>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Artist.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Artist.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>

    </tr>
}

</table>
